{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Deadlock Map Companion",
    "author": "Developer",
    "version": "1.0.1",
    "minimum-overwolf-version": "0.198.0",
    "description": "Tactical map analysis for Deadlock",
    "icon": "icon.png",
    "icon_gray": "icon_gray.png"
  },
  "permissions": [
    "GameInfo",
    "DesktopStreaming",
    "FileSystem"
  ],
  "data": {
    "start_window": "MainWindow",
    "windows": {
      "MainWindow": {
        "file": "index.html",
        "desktop_only": true,
        "native_window": true,
        "resizable": true,
        "transparent": false,
        "size": {
          "width": 1200,
          "height": 800
        },
        "min_size": {
          "width": 600,
          "height": 400
        }
      }
    }
  }
}