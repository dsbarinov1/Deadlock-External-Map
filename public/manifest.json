{
  "manifest_version": 1,
  "type": "WebApp",
  "meta": {
    "name": "Deadlock Map Companion",
    "author": "Developer",
    "version": "1.1.0",
    "minimum-overwolf-version": "0.198.0",
    "description": "Tactical map analysis for Deadlock",
    "icon": "icon.png",
    "icon_gray": "icon_gray.png"
  },
  "permissions": [
    "GameInfo",
    "DesktopStreaming",
    "FileSystem",
    "Hotkeys"
  ],
  "game_targeting": {
    "type": "dedicated",
    "game_ids": [24201]
  },
  "launch_events": [
    {
      "event": "GameLaunch",
      "start_minimized": false,
      "threshold": {
        "game_id": 24201
      }
    }
  ],
  "data": {
    "start_window": "MainWindow",
    "windows": {
      "MainWindow": {
        "file": "index.html",
        "desktop_only": true,
        "native_window": false,
        "resizable": true,
        "transparent": false,
        "size": {
          "width": 1200,
          "height": 800
        },
        "min_size": {
          "width": 600,
          "height": 400
        }
      }
    }
  }
}